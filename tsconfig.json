{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "screens/*": ["screens/*"],
      "styles/*": ["styles/*"],
      "hooks/*": ["hooks/*"],
      "services/*": ["services/*"],
      "@redux/*": ["redux/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["src"]
}
